{
  "name": "satisfactory-minmax",
  "version": "0.0.0-development",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "check-format": "prettier --list-different \"./**/*.{md,mts,ts,yml}\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,mts,ts}\"",
    "cz": "git-cz",
    "format": "prettier --write \"./{**/*.{md,mts,ts,yml},data/**/*.json}\"",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint . --ext .mts",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "prepare": "husky install",
    "start": "node --experimental-json-modules --enable-source-maps --experimental-loader esbuild-node-loader ./src/index.mts"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "fs-extra": "^10.0.0",
    "highs": "^0.4.7",
    "iterare": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@rebeccastevens/eslint-config": "^1.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "commitizen": "^4.2.4",
    "cspell": "^5.13.2",
    "esbuild-node-loader": "^0.6.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-ava": "^13.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^4.0.2",
    "eslint-plugin-functional": "^4.1.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "markdownlint-cli": "^0.30.0",
    "marked": "^4.0.10",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=17.0.0"
  }
}
